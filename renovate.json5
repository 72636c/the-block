{
  lockFileMaintenance: {
    enabled: true,
  },
  packageRules: [
    {
      matchUpdateTypes: [
        'major',
        'minor',
        'patch',
        'pin',
        'digest',
        'rollback',
        'bump',
      ],
      enabled: false,
    },
    {
      matchUpdateTypes: ['lockFileMaintenance'],
      automerge: true,
    },
    {
      matchUpdateTypes: ['lockFileMaintenance'],
      schedule: 'before 3:00 am every 2 weeks on Wednesday',
    },
  ],
}
