{
  extends: ['github>seek-oss/rynovate:non-critical'],
  packageRules: [
    {
      matchManagers: ['npm'],
      matchPackageNames: ['skuba'],
      matchUpdateTypes: ['major', 'minor', 'patch'],

      automerge: true,
      commitMessageExtra: '{{newValue}}',
      commitMessageTopic: '{{depName}}',
      groupName: null,
      prPriority: 99,
      schedule: 'before 3:00 am every weekday',
    },
  ],
}
