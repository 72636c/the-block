{
  "extends": [
    ":prHourlyLimit2",
    ":prNotPending",
    ":rebaseStalePrs",
    ":renovatePrefix",
    ":semanticCommits",
    ":timezone(Australia/Melbourne)",
    "preview:buildkite",
    "preview:dockerCompose"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "excludePackageNames": ["seek-jobs/gantry"],
      "groupName": "all dependencies",
      "recreateClosed": true,
      "schedule": "before 7am on Monday",
      "semanticCommitType": "fix",
      "updateTypes": [
        "bump",
        "digest",
        "major",
        "minor",
        "patch",
        "pin",
        "rollback"
      ]
    },
    {
      "commitMessageAction": "",
      "commitMessageExtra": "{{newValue}}",
      "commitMessageTopic": "{{depName}}",
      "packageNames": ["seek-jobs/gantry"]
    },
    {
      "automerge": true,
      "prPriority": 99,
      "schedule": "every 2 weeks on Wednesday",
      "updateTypes": ["lockFileMaintenance"]
    }
  ],
  "buildkite": {
    "commitMessageExtra": "",
    "managerBranchPrefix": ""
  },
  "docker": {
    "commitMessageExtra": "",
    "group": {
      "commitMessageTopic": "{{groupName}}"
    }
  },
  "commitMessageAction": "",
  "commitMessageExtra": "",
  "ignoreNpmrcFile": true,
  "lazyGrouping": false,
  "postUpdateOptions": ["yarnDedupeHighest"],
  "prConcurrentLimit": 2,
  "prNotPendingHours": 1,
  "schedule": "before 7am on every weekday",
  "semanticCommitScope": "",
  "semanticCommitType": "deps",
  "separateMajorMinor": false
}
